{
  "name": "mui-primitives",
  "version": "0.0.4-0",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "main": "./typescript-out/index.js",
  "types": "./typescript-out/index.d.ts",
  "files": [
    "typescript-out/**",
    "static-types.json"
  ],
  "scripts": {
    "dev": "yarn generate-static-types && concurrently -n \"web,play\"  -c \"magenta,greenBright\" \"next dev --port 2222\" \"OPEN_TAB=false playroom start\"",
    "build": "rimraf typescript-out && tsc --jsx react && yarn generate-static-types",
    "build-docs": "next build",
    "export-docs": "next export -o docs-out",
    "export-docs-and-serve": "yarn build-docs && yarn export-docs && serve docs-out --no-clipboard",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "playroom": "playroom start",
    "playroom:build": "playroom build",
    "generate-static-types": "./generate-static-types.sh > ./static-types.json",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "react-keyed-flatten-children": "^1.2.0"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@types/node": "^14.0.1",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "next": "^9.4.2",
    "playroom": "^0.18.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-element-to-jsx-string": "^14.3.1",
    "react-syntax-highlighter": "^12.2.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.1",
    "typescript": "^3.9.2"
  }
}
